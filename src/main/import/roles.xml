<?xml version="1.0" encoding="UTF-8"?>
<roles jcr:primaryType="jnt:roles"
       xmlns:jcr="http://www.jcp.org/jcr/1.0"
       xmlns:j="http://www.jahia.org/jahia/1.0"
       xmlns:jnt="http://www.jahia.org/jahia/nt/1.0">
    <editor jcr:primaryType="jnt:role">
        <editor-in-chief j:permissionNames="canSeeAdvancedOptionsTab setContentLimitsOnAreas"
                         j:privilegedAccess="true"
                         jcr:primaryType="jnt:role">
        </editor-in-chief>
    </editor>

    <owner jcr:primaryType="jnt:role" j:permissionNames="contentEditor"/>

    <server-administrator j:hidden="false"
                          j:nodeTypes="rep:root"
                          j:permissionNames="canSeeAdvancedOptionsTab"
                          j:privilegedAccess="true"
                          j:roleGroup="server-role"
                          jcr:primaryType="jnt:role">
    </server-administrator>

    <site-administrator j:hidden="false"
                        j:permissionNames="canSeeAdvancedOptionsTab setContentLimitsOnAreas"
                        j:privilegedAccess="true"
                        j:roleGroup="site-role"
                        jcr:primaryType="jnt:role">
    </site-administrator>
</roles>
